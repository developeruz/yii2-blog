{
	"name": "linchpinstudios/yii2-blog",
	"description": "Simple Blog for the Yii2 framework",
	"keywords": ["yii2", "blog"],
	"type": "yii2-extension",
	"license": "MIT",
	"support": {
		"issues": "https://github.com/linchpinstudios/yii2-blog/issues",
		"source": "https://github.com/linchpinstudios/yii2-blog"
	},
	"authors": [
		{
			"name": "Josh Hagel",
			"email": "joshhagel@linchpinstudios.com"
		}
	],
	"require": {
        "yiisoft/yii2": "~2.0",
		"linchpinstudios/yii2-datetimepicker": "dev-master",
		"linchpinstudios/yii2-filemanager": "dev-master",
		"2amigos/yii2-tinymce-widget" : "*"
	},
	"require-dev": {
		"yiisoft/yii2-codeception": "~2.0",
		"codeception/specify": "*",
		"codeception/verify": "*",
		"codeception/aspect-mock": "*"
	},
	"autoload": {
		"psr-4": {
			"linchpinstudios\\blog\\": ""
		}
	}
}
